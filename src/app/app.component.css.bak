ul {
    list-style-type: none;
    margin: 1;
    padding: 0;
    overflow: hidden;
    /* background-color: rgb(233, 238, 241); */
  }
  
  li {
    float: left;
  }
  
  li a {
    /* display: block; */
    color: rgb(15, 6, 41);
    text-align: center;
    padding: 14px 16px;
    text-decoration: wavy;
    font-size:large;
  }
  
  /* Change the link color to #111 (black) on hover */
  li a:hover {
    color: orange;
  }

  .active {
    background-color: #4CAF50;
  }

  
  .img{      
    margin:1px 1px;      
    border: 4px solid #eee;      
    box-shadow:rgb(224, 199, 199) 1px 1px 8px 1px;      
    cursor: pointer      
       }      
   .modal-content {      
     width: 1000px !important;      
   }      
